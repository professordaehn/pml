      "source": [
        "def square(x):\n",
        "    return x * x\n",
        "\n",
        "def times2(x):\n",
        "    return 2 * x\n",
        "\n",
        "def average(x1, x2, x3, x4, x5):\n",
        "    return (x1 + x2 + x3 + x4 + x5) / 5\n",
        "\n",
        "print(square(2))\n",
        "print(times2(2))\n",
        "print(average(1, 2, 3, 4, 5))\n"
      ],
